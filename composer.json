{
    "name": "laravel-liberu/people",
    "description": "Person management dependency for Laravel Liberu",
    "keywords": [
        "laravel-liberu",
        "people",
        "person-management"
    ],
    "homepage": "https://github.com/laravel-liberu/People",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/addresses": "^5.0",
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/dynamic-methods": "^2.0",
        "laravel-liberu/enums": "^2.0",
        "laravel-liberu/forms": "^4.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/migrator": "^2.0",
        "laravel-liberu/rememberable": "^3.0",
        "laravel-liberu/searchable": "^2.1",
        "laravel-liberu/track-who": "^2.0",
        "laravel-liberu/tables": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLiberu\\People\\": "src/",
            "LaravelLiberu\\People\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLiberu\\People\\AppServiceProvider",
                "LaravelLiberu\\People\\AuthServiceProvider",
                "LaravelLiberu\\People\\SearchServiceProvider"
            ]
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
